{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Prod2-Prod2",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/prod2-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsProd2/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Prod2",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Prod2/Prod2\"}"
      },
      "envFile": "${workspaceFolder}/Prod2/.env1",
      "preLaunchTask": "Build Prod2"
    }
  ]
}